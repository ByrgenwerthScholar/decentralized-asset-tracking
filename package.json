{
  "name": "brooklyn3.0",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run watch:backend\" \"npm run start:electron\"",
    "start:frontend": "cd src/frontend && npm start",
    "watch:backend": "cd src/backend/express && npm run watch",
    "start:electron": "cd src/electron && npm run start",
    "build": "npm run build:frontend && npm run build:backend && npm run build:electron",
    "build:frontend": "cd src/frontend && npm run build",
    "build:backend": "cd src/backend/express && npm run build",
    "build:electron": "cd src/electron && npm run build"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
